#include <bits/stdc++.h>
using namespace std;
int main()
{
    int N;
    cin>>N;
    // cout<<N<<endl;
    while(N--)
    {
        int a;
        cin>>a;
        int C[a];
        for (int i=0; i<a; i++) cin>>C[i];
        if (next_permutation(C,C+a)) for (int i=0; i<a; i++) cout<<C[i]<<' ';
        else for (int i=0; i<a; i++) cout<<i+1<<' ';
        cout<<endl;
    }
}
